{
  "name": "@toolkit-dev/tsconfig",
  "version": "0.0.1",
  "description": "TypeScript configuration for the repo",
  "type": "module",
  "license": "MIT",
  "exports": {
    "./*.json": "./*.json"
  },
  "scripts": {
    "clean:deps": "rm -rf ./node_modules",
    "format": "prettier --write .",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "pnpm lint --fix"
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "typescript": "5.7.3"
  }
}
